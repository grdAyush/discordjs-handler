generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

model UserId {
  id        BigInt   @id @default(autoincrement())
  noPrefix  NoPrefix @relation(fields: [noPrefixId], references: [id])
  noPrefixId BigInt
}

model NoPrefix {
  id    BigInt   @id @default(autoincrement())
  users UserId[]
}

